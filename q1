#include <bits/stdc++.h>
using namespace std;

int main() 
{
    int a,b;
    cin>>a>>b;
    string y[10]={"one","two","three","four","five","six","seven","eight","nine"};
    int i = a;
    while(i<=b)
    {
        if(i<=9)
        {
            cout<<y[i-1]<<endl;
        }
        else
        {
            if(i%2==0)
            {
                cout<<"even"<<endl;
            }
            else
            {
                cout<<"odd"<<endl;
            }
        }
        i++;
    }
    return 0;
}
